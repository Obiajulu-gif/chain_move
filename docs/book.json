{
  "title": "ChainMove Documentation",
  "description": "Comprehensive documentation for ChainMove - Decentralized Vehicle Financing Platform",
  "author": "ChainMove Team",
  "language": "en",
  "plugins": [
    "theme-api",
    "prism",
    "-highlight",
    "hints",
    "expandable-chapters",
    "mermaid-gb3",
    "toggle-chapters",
    "codeblock-filename",
    "theme-default-extend"
  ],
  "pluginsConfig": {
    "theme-default-extend": {
      "showLevel": true
    },
    "theme-api": {
      "theme": "dark"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    }
  },
  "styles": {
    "website": "./styles/website.css"
  },
  "variables": {
    "themeColor": "#4F46E5"
  }
}
